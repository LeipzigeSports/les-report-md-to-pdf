#import "@preview/based:0.2.0": base64

#set text(
  font: ("Saira", "Noto Color Emoji"),
  size: 12pt,
  lang: "de"
)

#set page(
  paper: "a4",
  margin: (left: 2.5cm, right: 2.5cm, top: 4cm, bottom: 1.5cm),
  header: [
    #grid(
      columns: (1fr, auto),
      [
        Divisionsbericht $team$ #datetime.today().display("[month repr:long] [year]") \
        Stand #datetime.today().display("[day].[month].[year]") | Leipzig eSports e.V.
      ],
      [
        #image(
          base64.decode(""),
          height: 1cm
        )
      ]
    )
    #line(length: 100%)
  ]
)

#set heading(
  numbering: "1.1"
)

$body$
